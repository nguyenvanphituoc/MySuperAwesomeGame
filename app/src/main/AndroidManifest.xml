<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.mysuperawesomegame"
    android:installLocation="preferExternal">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.dell.mysuperawesomegame.jumper.SuperJumper"
            android:configChanges="keyboard|keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>


            <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dell.mysuperawesomegame.MainActivity"
            android:configChanges="keyboard|keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>

                <!-- <action android:name="android.intent.action.MAIN" /> -->

                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.AndroidBasicsStarter">
            <intent-filter>

                <category android:name="android.intent.category.DEFAULT" />

            </intent-filter>
        </activity>
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.FirstTriangleTest">
            <intent-filter>
                <action android:name="com.badlogic.androidgames.framework.LAYOUT.FIRSTTRIANGLETEST"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.IndexedTest">
            <intent-filter>
                <action android:name="com.badlogic.androidgames.framework.LAYOUT.INDEXEDTEST"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.GLGameTest"/>
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.GLSurfaceViewTest"/>
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.LifeCycleTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.SingleTouchTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.MultiTouchTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.KeyTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.AccelerometerTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.AssetsTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.ExternalStorageTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.SoundPoolTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.MediaPlayerTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.FullScreenTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.RenderViewTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.ShapeTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.BitmapTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.FontTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.SurfaceViewTest" />
        <activity android:name="com.badlogic.androidgames.framework.LAYOUT.AnimationTest"/>
        <activity android:name="com.example.dell.mysuperawesomegame.mrnom2d.MrNomGame"
            android:screenOrientation="portrait"
            android:configChanges="keyboard|keyboardHidden|orientation">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>